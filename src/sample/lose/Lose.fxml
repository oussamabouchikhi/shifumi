<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="lose-background" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.lose.Lose">
   <children>
      <Label layoutX="122.0" layoutY="84.0" text="You lost :(" textFill="WHITE">
         <font>
            <Font name="Arial Black" size="64.0" />
         </font>
      </Label>
      <JFXButton buttonType="RAISED" layoutX="200.0" layoutY="323.0" onAction="#replay" styleClass="again-lose" stylesheets="@../css/style.css" text="Play again">
         <font>
            <Font name="Arial" size="22.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="quitBtn" buttonType="RAISED" layoutX="355.0" layoutY="323.0" onAction="#quit" styleClass="again-lose-reversed" stylesheets="@../css/style.css" text="Quit">
         <font>
            <Font name="Arial" size="22.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
